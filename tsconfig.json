{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"lib": ["ES2020", "WebWorker"],
		"types": ["@cloudflare/workers-types"],
		"moduleResolution": "node",
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["worker/**/*.ts"],
	"exclude": ["node_modules", "dist", "webapp"]
}
